<html>
	<head>


		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			{% block current_css %}{% endblock %}
		{% endblock %}


		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			{% block current_javascript %}{% endblock %}
		{% endblock %}



		<title>
			{% block title %}Milao
			{% endblock %}
		</title>


		<meta name="description" content="Welcome in Milao ressel ticket and merch and give news">
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
		<meta charset="UTF-8">
	</head>
	<body>
		<header>
			<nav id="navbar">
				<a href="{{ path('app_homepage') }}" class="logo" aria-label="visit homepage" aria-current="page">
				<img id="bzzzIcon" src="{{ asset('./img/fly_PNG3950.png')}}" alt="Bzzz icon">
					<span>
						Flies everywhere
					</span>
				</a>
				<div class="nav-links">
					<button class="hamburger" type="button" aria-label="Toggle navigation" aria-expanded="false">

					</button>
				</div>
				<div class="navlinks-container">
					<a href="{{ path('app_merch') }}">Merch</a>
					<a href="{{ path('app_dates') }}">Dates</a>
					<a href="{{ path('app_contact') }}">Contact</a>
					<a href="{{ path('app_help') }}">1312</a>
				</div>
				<div>

					<div>


						<div class="signs-btns">
							<a href="{{ path('app_register') }}">

								<button type="button" id="button-register">Sign In</button>
							</a>
							<a href="{{ path('app_security') }}">
								<button type="button" id="button-login">Sign Up</button>
							</a>
						</div>
					</div>
				</div>
			</nav>
		</header>
	<main>
		{% block body %}{% endblock %}
	</main>	
		
			{% include 'footer/footer.html.twig' %}
		
	</body>



</html>
